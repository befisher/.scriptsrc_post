# Simplify commands.
alias ..="cd ../";
alias ...="cd ../../";
alias ....="cd ../../../";
alias .....="cd ../../../../";
alias ......="cd ../../../../../";
alias cd1="cd ..";
alias cd2="cd ../..";
alias cd3="cd ../../..";
alias cd4="cd ../../../..";
alias cd5="cd ../../../../..";

# For frequently used commands.
alias c="clear";
alias r="reset";
alias q="exit";

# Hint for important operations. 
alias rm="rm -v";
alias cp="cp -v";



